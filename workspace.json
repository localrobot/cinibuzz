{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "cinibuzz",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "babel": true,
        "strict": true,
        "e2eTestRunner": "none",
        "routing": false,
        "unitTestRunner": "none"
      },
      "component": {
        "style": "none",
        "flat": true,
        "skipTests": true
      },
      "library": {
        "style": "@emotion/styled",
        "linter": "eslint",
        "strict": true,
        "buildable": true,
        "component": false,
        "unitTestRunner": "none"
      }
    }
  },
  "projects": {
    "cinibuzz": {
      "root": "apps/cinibuzz",
      "sourceRoot": "apps/cinibuzz/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/cinibuzz",
            "index": "apps/cinibuzz/src/index.html",
            "main": "apps/cinibuzz/src/main.tsx",
            "polyfills": "apps/cinibuzz/src/polyfills.ts",
            "tsConfig": "apps/cinibuzz/tsconfig.app.json",
            "assets": ["apps/cinibuzz/src/favicon.ico", "apps/cinibuzz/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/cinibuzz/src/environments/environment.ts",
                  "with": "apps/cinibuzz/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "cinibuzz:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "cinibuzz:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cinibuzz/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    }
  }
}
